{
  "routes": [
    {
      "src": "/npm/(.*)",
      "dest": "https://fastly.jsdelivr.net/npm/$1"
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "src": "/gh/(.*)",
      "dest": "https://fastly.jsdelivr.net/gh/$1"
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}

